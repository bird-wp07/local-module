{
    "name": "@bird-wp07/local-module",
    "version": "0.0.44",
    "license": "",
    "bin": "./bin/start",
    "files": [
        "bin/",
        "dist/src/",
        "dist/generated/",
        "package.json",
        "README.md"
    ],
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "test": "npm run build && npm run test:nobuild",
        "test0": "npm run build && ./scripts/run-tests-selfcontained.sh",
        "test:nobuild": "scripts/run-tests.sh",
        "test:debug": "npm run build && scripts/run-tests.sh -d",
        "build:clean": "rm -rf ./dist ./generated",
        "build:version.ts": "mkdir -p generated && node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > generated/version.ts",
        "build:tsoa": "npx tsoa spec-and-routes",
        "build": "npm run build:clean && npm run build:version.ts && npm run build:tsoa && npx tsc -p ./tsconfig.json",
        "start": "npm run build && node ./dist/src/main.js",
        "start:debug": "npm run build && NODE_OPTIONS=--inspect-brk node ./dist/src/main.js",
        "lint": "npm run build && npx eslint 'src/**/*.ts' 'tests/**/*.ts'",
        "publish_npmjs": "npm run build && npm publish",
        "release": "./scripts/release.sh"
    },
    "dependencies": {
        "@lapo/asn1js": "^1.2.4",
        "@peculiar/asn1-cms": "^2.3.4",
        "@peculiar/asn1-schema": "^2.3.3",
        "@tsoa/runtime": "^4.1.3",
        "@types/node": "^18.11.9",
        "axios": "^1.2.2",
        "express": "^4.18.2",
        "fs": "^0.0.1-security",
        "neverthrow": "^5.1.0",
        "swagger-ui-express": "^4.6.0",
        "tsoa": "^4.1.3",
        "typescript": "^4.9.3",
        "wait-for-event": "^2.0.1",
        "winston": "^3.8.2",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/chai-subset": "^1.3.3",
        "@types/express": "^4.17.14",
        "@types/lapo__asn1js": "^1.2.1",
        "@types/mocha": "^10.0.0",
        "@types/multer": "^1.4.7",
        "@types/newman": "^5.3.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/xml2js": "^0.4.11",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "chai": "^4.3.6",
        "chai-subset": "^1.6.0",
        "enhanced-publish": "^1.1.1",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-neverthrow": "^1.1.4",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.1.0",
        "newman": "^5.3.2",
        "prettier": "^2.8.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "typescript-eslint": "^0.0.1-alpha.0"
    }
}

{
  "openapi": "3.0.3",
  "info": {
    "title": "WP07 Local Module - OpenAPI 3.0",
    "description": "",
    "version": "0.2.0"
  },
  "paths": {
    "/digest/pdf": {
      "post": {
        "summary": "Returns digest to be signed of an unsigned PDF",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "bytes",
                  "signingTimestamp"
                ],
                "type": "object",
                "properties": {
                  "bytes": {
                    "type": "string",
                    "description": "Base64 encoded PDF",
                    "example": ""
                  },
                  "signingTimestamp": {
                    "type": "number",
                    "description": "Timestamp in milliseconds since epoch",
                    "example": 1676906127623
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "bytes": {
                      "type": "string",
                      "description": "Base64 encoded SHA256 digest to be signed",
                      "example": "4LedqdTMbBbnZF9YjfxVkIZYbG51lgYuubiNp9EqjS4="
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/issue": {
      "post": {
        "summary": "Issues a signature",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "bytes",
                  "issuerId"
                ],
                "type": "object",
                "properties": {
                  "bytes": {
                    "type": "string",
                    "description": "Base64 encoded SHA256 digest to be signed",
                    "example": "4LedqdTMbBbnZF9YjfxVkIZYbG51lgYuubiNp9EqjS4="
                  },
                  "issuerId": {
                    "type": "string",
                    "description": "Issuer ID used to identify institution or staff member requesting issuance",
                    "example": "8d51fa75-b98e-4d8f-98f1-dee5d471a450"
                  },
                  "auditLog": {
                    "type": "string",
                    "description": "Optional audit log entry",
                    "example": "[2023-04-01T12:00:00Z] School #314, Staff #331.A"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "cms": {
                      "$ref": "#/components/schemas/Cms"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/merge/pdf": {
      "post": {
        "summary": "Produces a signed PDF by merging the original PDF with its signature.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "bytes",
                  "signingTimestamp",
                  "cms"
                ],
                "type": "object",
                "properties": {
                  "bytes": {
                    "type": "string",
                    "description": "Base64 encoded original PDF used to generate the digest to be signed",
                    "example": ""
                  },
                  "signingTimestamp": {
                    "type": "number",
                    "description": "Original signing timestamp used to generate the digest to be signed",
                    "example": 1676906127623
                  },
                  "cms": {
                    "$ref": "#/components/schemas/Cms"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "bytes": {
                      "type": "string",
                      "description": "Base64 encoded signed PDF"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/validate/pdf": {
      "post": {
        "summary": "Validates a signed PDF's signature",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "bytes"
                ],
                "type": "object",
                "properties": {
                  "bytes": {
                    "type": "string",
                    "description": "Base64 encoded signed PDF"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "valid": {
                      "type": "boolean",
                      "description": "Overall validation result. Returns true if and only if all aspects of the validation are successful."
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Cms": {
        "type": "string",
        "description": "Base64 encoded signature in CMS format",
        "example": ""
      }
    }
  }
}